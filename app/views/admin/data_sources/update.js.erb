$(document).ready(function(){
  $.ajax({
    beforeSend:function(respond){
    },
    complete:function(respond){
      <%- if @data_source.errors.any? %>
        $('#support-data-source').html('<%= escape_javascript(
          render('/admin/data_sources/form', :url => admin_data_sources_path,
            :data_source => @data_source, :title => "Ubah Sumber Data",
            :name_button => GlobalClass.button_update)) %>');
      <% else %>
        parent.$.fancybox.close();
      <% end %>
    }
  });
});
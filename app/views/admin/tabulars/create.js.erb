$(document).ready(function(){
  $.ajax({
    beforeSend:function(respond){
    },
    complete:function(respond){
      <%- if @tabular.errors.any? %>
        $('#support-data').html('<%= escape_javascript(
          render('/admin/tabulars/form', :url => admin_tabulars_path, :tabular => @tabular,
            :unit_options => @unit_options, :options_data_sources => @options_data_sources,
            :tabular_options => @tabular_options, :title => "Tambah Tabular Data",
            :name_button => "Simpan"))
          %>');
      <% else %>
        parent.$.fancybox.close();
      <% end %>
    }
  });
});
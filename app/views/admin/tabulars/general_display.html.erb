<div class="content">
  <div class="page-header">
    <div class="icon">
      <span class="ico-layout-7"></span>
    </div>
    <h1>Data Tabular <small>ADMINISTRASI SISTEM INFORMASI PEMBANGUNAN DAERAH</small></h1>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <div class="block">
        <div class="data-fluid">
          <table cellpadding="0" cellspacing="0" width="100%" class="table">
            <thead>
              <tr>
                <th width="50%">
                  Nama Sumber Data
                </th>
                <th width="10%">
                  Value
                </th>
                <th width="10%">
                  Satuan
                </th>
                <th width="25%">
                  Sumber Data
                </th>
              </tr>
            </thead>
            <tbody>
              <%= nested_master_tabulars(@tabulars.arrange(:order => :created_at)) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
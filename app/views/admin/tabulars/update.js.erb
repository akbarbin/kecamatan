$(document).ready(function(){
  $.ajax({
    beforeSend:function(respond){
    },
    complete:function(respond){
      <%- if @tabular.errors.any? %>
        $('#support-data').html('<%= escape_javascript(
          render('/admin/tabulars/form', :url => admin_tabulars_path,
            :tabular => @tabular, :option_tabulars => @option_tabulars,
            :options_data_sources => @options_data_sources,
            :title => "Tambah Tabular Data", :name_button => "Simpan"))
          %>');
      <% else %>
        parent.$.fancybox.close();
      <% end %>
    }
  });
});
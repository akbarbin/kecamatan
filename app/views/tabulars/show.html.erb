<div class="content">
  <div class="page-header">
    <div class="icon">
      <span class="ico-layout-7"></span>
    </div>
    <h1>Lihat Tabular <small>ADMINISTRASI SISTEM INFORMASI PEMBANGUNAN DAERAH</small></h1>
  </div>
  <div class="span3">
    <p><strong>Kecamatan:</strong> <%= @tabular.user_name %></p>
    <p><strong>Tahun:</strong> <%= @tabular.year %></p>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <div class="block">
        <div class="data-fluid">
          <%= link_to "Edit", "#", :class => "btn edit-tabular" %>
          <%= form_for @tabular, :url => update_all_tabulars_path, :html => {:method => :put} do |f| %>
            <%= nested_tabulars(@user_tabulars.arrange(:order => :created_at), f: f) %>
            <%= f.submit :class => "update-all-btn-tabular" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>